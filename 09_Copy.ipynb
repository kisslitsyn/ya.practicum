    "    .apply(lambda x: x['address'] + x['object_name'] + str(x['seats']), axis = 1)\n",
    "\n",
    "# количество дубликатов по столбцу address_name_seats\n",
    "len(rest_data[rest_data['address_name_seats'].duplicated(keep = False)])"
   ]
